Host bastion
  Hostname bastion.vault-demo.useless.mobi
  User admin
  IdentityFile ./keys/k8s/kubernetes_rsa
  PasswordAuthentication no
  ForwardAgent yes
  ServerAliveInterval 60
  TCPKeepAlive yes
  ControlMaster auto
  ControlPath ~/.ssh/ansible-%r@%h:%p
  ControlPersist 15m
  LogLevel QUIET

Host 90.80.*
  User admin
  IdentityFile ./keys/k8s/kubernetes_rsa
  ServerAliveInterval 60
  TCPKeepAlive yes
  ProxyJump admin@bastion
  LogLevel QUIET
  StrictHostKeyChecking no